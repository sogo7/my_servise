<div class="container">
    <%= render 'new' %>
    <%= link_to "自分の大喜利を見る", "#" %><br />
    <%= link_to "人気", "#" %><br />
    <%= link_to "新着", "#" %><br />
    <%= link_to "お気に入りの大喜利", "#" %><br />

    <% @posts.each do |post| %>
        <div class="post-image">
            <%= link_to image_tag(post.img.to_s, size: "400"), post_path(post) %><br />
        </div>
        <%= post.user.id %><br />
        <div id="like-btn-<%= post.id %>">
            <%= render "goods/good", post: post %>
        </div>
        <%= post.ogiri %><br />

        <% post.comments.each do |comment| %>
            <div id="like-btn-<%= post.comment.id %>">
                <%= render "goods/good", post: post %>
            </div>
            <%= comment.ogiri %><br />
        <% end %>
    <% end %>
    

</div>
